<div class="container">

  <div class="row">
<% if user_signed_in? %>


<!-- Left Column -->
  <%= render "frames/side" %>

<!-- Main body -->

    <div class="eight columns frame">


    <h1><%= @team.name.capitalize %></h1>


    </div>

<% else %>


<!-- Main body -->

    <div class="eleven columns frame">


	<h2>You are not logged in </h2>
		<%= link_to "Login", new_user_session_path %><br/>
		<%= link_to "Signup", new_user_registration_path %>


    </div>



<% end %>


  </div>

</div>
